<div id="embed_stuff">
	<script>yam.connect.actionButton({
	  container: "#embed-like",
	  action: "like",
	  <% if @open_graph_object.embedlike_network %>network: "<%= @open_graph_object.embedlike_network %>",<% end %>

	  <% if @open_graph_object.embedlike_op_url || @open_graph_object.embedlike_op_type || @open_graph_object.embedlike_op_title || @open_graph_object.embedlike_op_image %>
	  objectProperties: {
		<% if @open_graph_object.embedlike_op_url %>url: "<%= @open_graph_object.embedlike_op_url %>",<% end %>
		<% if @open_graph_object.embedlike_op_type %>type: "<%= @open_graph_object.embedlike_op_type %>",<% end %>
		<% if @open_graph_object.embedlike_op_title %>title: "<%= @open_graph_object.embedlike_op_title %>",<% end %>
		<% if @open_graph_object.embedlike_op_image %>image: "<%= @open_graph_object.embedlike_op_image %>",<% end %>
	  }
	  <% end %>
	});</script>

	<script>yam.connect.actionButton({
	  container: "#embed-follow",
	  action: "follow",
	  <% if @open_graph_object.embedfollow_network %>network: "<%= @open_graph_object.embedfollow_network %>",<% end %>

	  <% if @open_graph_object.embedfollow_op_url || @open_graph_object.embedfollow_op_type || @open_graph_object.embedfollow_op_title || @open_graph_object.embedfollow_op_image %>
	  objectProperties: {
		<% if @open_graph_object.embedfollow_op_url %>url: "<%= @open_graph_object.embedfollow_op_url %>",<% end %>
		<% if @open_graph_object.embedfollow_op_type %>type: "<%= @open_graph_object.embedfollow_op_type %>",<% end %>
		<% if @open_graph_object.embedfollow_op_title %>title: "<%= @open_graph_object.embedfollow_op_title %>",<% end %>
		<% if @open_graph_object.embedfollow_op_image %>image: "<%= @open_graph_object.embedfollow_op_image %>",<% end %>
	  }
	  <% end %>
	});</script>

	<script>yam.connect.embedFeed({
	  container: "#embed-feed",
	  feedType: "open-graph",
	  <% if @open_graph_object.embedfeed_network %>network: "<%= @open_graph_object.embedfeed_network %>",<% end %>

	  <% if @open_graph_object.embedfeed_op_url || @open_graph_object.embedfeed_op_type || @open_graph_object.embedfeed_op_title || @open_graph_object.embedfeed_op_image %>
	  objectProperties: {
		<% if @open_graph_object.embedfeed_op_url %>url: "<%= @open_graph_object.embedfeed_op_url %>",<% end %>
		<% if @open_graph_object.embedfeed_op_type %>type: "<%= @open_graph_object.embedfeed_op_type %>",<% end %>
		<% if @open_graph_object.embedfeed_op_title %>title: "<%= @open_graph_object.embedfeed_op_title %>",<% end %>
		<% if @open_graph_object.embedfeed_op_image %>image: "<%= @open_graph_object.embedfeed_op_image %>",<% end %>
	  }
	  <% end %>
	});</script>

	<h2>Embed Like Button</h2>
	<div id="embed-like"></div>
	<h2>Embed Follow Button</h2>
	<div id="embed-follow"></div>
	<h2>Embed Object Feed</h2>
	<div id="embed-feed"></div>
</div>
