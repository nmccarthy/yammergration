<% if @staging %>
	<script data-app-id="<%= @yammergration_yammer_staging_client_id %>" src="https://assets.staging.yammer.com/platform/yam.js"></script>
<% else %>
	<script data-app-id="<%= @yammergration_yammer_production_client_id %>" src="https://assets.yammer.com/platform/yam.js"></script>
<% end %>

<p id="notice"><%= notice %></p>

<% content_for :title, @open_graph_object.title %>
<% content_for :og_site_name, @open_graph_object.og_site_name %>
<% content_for :og_image, @open_graph_object.og_image %>
<% content_for :og_description, @open_graph_object.og_description %>
